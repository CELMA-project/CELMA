    "fx3FDT = fx3FD.subs([(f(x+h/2), Fp05),\n",
    "                     (f(x)    , F   ),\n",
    "                     (f(x-h)  , Fm1 ),\n",
    "                     (f(x-2*h), Fm2 ),\n",
    "                     (f(x-3*h), Fm3 ),\n",
    "                     (f(x-4*h), Fm4 ),                     \n",
    "                    ])\n",
    "display(simplify(fx3FDT))"
   ]
